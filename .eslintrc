{
  "root": true,
  "failOnError": true,
  "failOnWarning": false,
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb"
  ],
  "rules": {
    "arrow-body-style": 2,
    "indent": ["error", 4, { "SwitchCase": 1 }],
    "max-len": ["error", 120, { "ignoreStrings": true, "ignoreComments": true }],
    "new-cap": 2,
    "no-bitwise": 2,
    "no-console": 2,
    "no-param-reassign": 0,
    "no-useless-constructor": 2,
    "object-curly-spacing": 2,
    "prefer-const": 2,
    "prefer-template": 2,
    "quote-props": 2,
    "no-var": 2,
    "quotes": ["error", "single", { "allowTemplateLiterals": true }],
    "semi": 2,
    "wrap-iife": [ "error", "outside" ],
    "one-var": 2,

    "array-callback-return": 1,
    "class-methods-use-this": 1,
    "default-case": 1,
    "no-mixed-operators": 1,
    "no-multi-assign": 1,
    "no-restricted-syntax": 1,

    "strict": 0,
    "object-shorthand": 0,
    "consistent-return": 0,
    "dot-notation": 0,
    "eqeqeq": 0,
    "func-names": 0,
    "comma-dangle": 0,
    "no-multi-spaces": 0,
    "no-plusplus": 0,
    "no-prototype-builtins": 0,
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "vars-on-top": 0,

    "padded-blocks": 0,
    "prefer-arrow-callback": 0,
    "prefer-rest-params": 0,
    "react/jsx-space-before-closing": 0,
    "spaced-comment": 0
  },
  "globals": {
    "$": true,
    "$uibModal": true,
    "_": true,
    "angular": true,
    "jQuery": true,
    "LoadingStatusManager": true,
    "moment": true,
    "notifyAboutUnSavedData": true,
    "Raven": true,
    "Rx": true,
    "sb": true,
    "sbMessage": true,
    "SearchGroup": true
  }
}